{
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "name": "Cute QR Code Scanner",
  "short_name": "Cute QR",
  "start_url": "/",
  "categories": ["qr", "code", "scanner", "privacy"],
  "display": "standalone",
  "background_color": "rgb(236, 253, 245)",
  "theme_color": "rgb(236, 253, 245)",
  "description": "The cutest QR code scanner on the web. No ads, no tracking, no data collection.",
  "orientation": "portrait",
  "icons": [
    {
      "src": "./maskable_icon-48.png",
      "sizes": "48x48",
      "type": "image/png"
    },
    {
      "src": "./maskable_icon-72.png",
      "sizes": "72x72",
      "type": "image/png"
    },
    {
      "src": "./maskable_icon-96.png",
      "sizes": "96x96",
      "type": "image/png"
    },
    {
      "src": "./maskable_icon-128.png",
      "sizes": "128x128",
      "type": "image/png"
    },
    {
      "src": "./maskable_icon-256.png",
      "sizes": "256x256",
      "type": "image/png"
    },
    {
      "src": "./maskable_icon-512.png",
      "sizes": "512x512",
      "type": "image/png"
    },
    {
      "src": "./maskable_icon-1024.png",
      "sizes": "1024x1024",
      "type": "image/png"
    }
  ]
}
